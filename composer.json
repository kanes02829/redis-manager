{
    "name": "encore/redis-manager",
    "description": "Redis manager for laravel",
    "type": "library",
    "keywords": ["laravel", "redis", "manager"],
    "homepage": "https://github.com/z-song/redis-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/contracts": "5.5.*",
        "illuminate/support": "5.5.*",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/laravel": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\RedisManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\RedisManager\\RedisManagerServiceProvider"
            ]

        }
    }
}
